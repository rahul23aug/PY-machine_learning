      "source": [
        "Yp = X_data.dot(res)\n",
        "plt.plot(np.arange(Yp.shape[0]), Yp,color='b')\n",
        "plt.scatter(np.arange(Yp.shape[0]), y_data,color='r')\n",
        "plt.xlabel(\"Iterations\")\n",
        "plt.ylabel(\"Residuals\")\n",
        "plt.show()"
