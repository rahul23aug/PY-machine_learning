[]\n"," for i in range(iter):\n","  dataset_x=dataset.copy()\n","  yp=(dataset_x * thetas).sum(axis=1)\n","  R=yp-y_response\n","  rl.append((R.sum())**2)\n","  diff=(dataset_x.T*R).sum(axis=1)\n","  thetas = thetas - (alpha*diff)/dataset.shape[0]\n","#create polynomial features\n","  for count in range(dataset_x.shape[1]):\n","   dataset_x[count] = dataset_x[count] ** (count+1)\n","#Plotting the result\n"," X=np.arange(iter)\n"," plt.plot(x_data,yp)\n"," plt.scatter(x_data,y_data, color='r')\n"," plt.xlabel(\"X data\",color='orange')\n"," plt.ylabel(\"Y data\",color='orange')\n"," plt.show()\n"," MSE= rl[-1] / dataset.shape[0]\n"," print(\"MSE: %.5f\"%MSE)\n"," print(\"Thetas: \\n\",thetas)\n"," return rl,iter\n","res=polynomial_regression(x_data, y_data, 2, 0.03,30)"],"execution_count":null,"outputs":[{"output_type":"display_data","data":{"image/png":"\n","text/plain":["<Figure size 432x288 with 1 Axes>"]},"metadata":{"tags":[],"needs_background":"light"}},{"output_type":"stream","text":["MSE: 0.03744\n","Thetas: \n"," 0    0.714164\n","1    0.123258\n","dtype: float64\n"],"name":"stdout"}]},{"cell_type":"code","metadata":{"id":"LpnpdxAnEs-u","colab_type":"code","colab":{"base_uri":"https://localhost:8080/","height":218},"executionInfo":{"status":"ok","timestamp":1596824069673,"user_tz":-330,"elapsed":1098,"user":{"displayName":"Rahul Vanjeri","photoUrl":"https://lh3.googleusercontent.com/a-/AOh14GgH1HSz0JKs1LQj-04INYky5897FXslGDXH0pExRQ=s64","userId":"14353470640849868231"}},"outputId":"df237506-d858-4577-c832-4bb54d80d191"},"source":["plt.plot(np.arange(res[1]), res[0],color='orange')\n","plt.xlabel(\"Iterations\",color='orange')\n","plt.ylabel(\"Residuals\",color='orange')\n","plt.show()"],"execution_count":null,"outputs":[{"output_type":"display_data","data":{"image/png":"\n","text/plain":["<Figure size 432x288 with 1 Axes>"]},"metadata":{"tags":[],"needs_background":"light"}}]}]}